<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="lukewireBlog.Domain.NavigationView.Index"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:lukewireBlog="clr-namespace:lukewireBlog"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:lukewireBlog.Domain.NavigationView"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Viewbox Margin="50" Stretch="Uniform">
        <TabControl Margin="5">
            <TabItem Header="BrandNavigationView">
                <views:BrandNavigationView DataContext="{Binding BrandViewModel}" />
            </TabItem>
            <TabItem Header="BottomNavigatorBarStyle2">
                <views:BottomNavigatorBarStyle2View DataContext="{Binding BottomNavigationBarStyle2ViewModel}" />
            </TabItem>
        </TabControl>
    </Viewbox>
</UserControl>
