<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="lukewireBlog.Components.Pages.DashBoard"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:shared="clr-namespace:lukewireBlog.Components.Shared"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <UserControl.Styles>
        <StyleInclude Source="avares://lukewireBlog/Components/Shared/ContentBox.axaml" />
    </UserControl.Styles>
    <shared:ContentBox ItemsSource="{Binding ContentItems}" />
</UserControl>
