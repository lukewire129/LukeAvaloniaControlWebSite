<UserControl
    MinHeight="500"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="lukewireBlog.Domain.Home.Index"
    xmlns="https://github.com/avaloniaui"
    xmlns:components="clr-namespace:lukewireBlog.Components"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <UserControl.Styles>
        <StyleInclude Source="avares://lukewireBlog/Components/BlogPanel.axaml" />
    </UserControl.Styles>
    <components:BlogPanel ItemsSource="{Binding Items}" />
</UserControl>
